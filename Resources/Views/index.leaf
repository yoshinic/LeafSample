<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>#(title)</title>
  </head>
  <body>
    <p>#(title)</p>

    <table>
      <thead>
        <tr>
          <th>番号</th>
          <th>名前</th>
        </tr>
      </thead>
      <tbody>
        #for(name in a):
          <tr>
            <td>#index(a, name)</td>
            <td>#(name)</td>
          </tr>
        #endfor
      </tbody>
    </table>

  </body>
</html>